 .timescale -9 -12;
v000000000094e570_0 .var "oscillate", 0 0;
    .scope S_0000000000842b50;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000000000094e570_0, 0, 1;
T_0.0 ;
