#! /usr/local/iverilog/bin/vvp
:ivl_version "11.0 (devel)" "(s20150603-642-g3bdb50da)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "vhdl_textio";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_00000000010e2b50 .scope module, "stimulus" "stimulus" 2 3;
 .timescale -9 -12;
v00000000010e2ce0_0 .var "clock", 0 0;
    .scope S_00000000010e2b50;
T_0 ;
    %vpi_call 2 6 "$dumpfile", "output.vcd" {0 0 0};
    %vpi_call 2 7 "$dumpvars", 32'sb00000000000000000000000000000000, S_00000000010e2b50 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v00000000010e2ce0_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_00000000010e2b50;
T_1 ;
    %delay 30000, 0;
    %load/vec4 v00000000010e2ce0_0;
    %inv;
    %store/vec4 v00000000010e2ce0_0, 0, 1;
    %delay 10000, 0;
    %load/vec4 v00000000010e2ce0_0;
    %inv;
    %store/vec4 v00000000010e2ce0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_00000000010e2b50;
T_2 ;
    %vpi_call 2 15 "$monitor", "time = %3d clock = %d", $time, v00000000010e2ce0_0 {0 0 0};
    %end;
    .thread T_2;
    .scope S_00000000010e2b50;
T_3 ;
    %delay 500000, 0;
    %vpi_call 2 17 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 3;
